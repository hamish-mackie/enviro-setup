alias gg='git grep'

# Tmux aliases
alias tmuxns="tmux new -s"
# Attach to a session: tas <session_name>
alias tmuxas="tmux attach-session -t"
# Kill a session: tks <session_name>
alias tmuxks="tmux kill-session -t"
# Rename a session: trs <old_name> <new_name>
alias tmuxrs="tmux rename-session -t"
# List tmux sessions: tmux list-sessions
alias tmuxls="tmux ls"
